<hibernate-mapping>
    <class name="com.excelr.collection.entity.PlayerTeam" table="player_team">
        <id name="id" type="java.lang.Long">
            <column name="id" />
            <generator class="identity" />
        </id>

        <many-to-one name="player" class="com.excelr.collection.entity.Player" column="player_id" />
        <many-to-one name="team" class="com.excelr.collection.entity.Team" column="team_id" />
    </class>
</hibernate-mapping>